{
  "name": "package",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\"",
    "clean": "rimraf distribution",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "imagemin": "imagemin img/* -o dist/img",
    "usemin": "usemin contactus.html -d dist --htmlmin -o dist/contactus.html && usemin index.html -d dist --htmlmin -o dist/index.html && usemin aboutus.html -d dist --htmlmin -o dist/aboutus.html",
    "build": "npm run clean && npm run copyfonts && npm run imagemin && npm run usemin"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "lite-server": "^2.4.0",
    "liteserver": "^0.3.0",
    "node-sass": "^4.9.0",
    "onchange": "^4.0.0",
    "parallelshell": "^3.0.2",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mehtadhiraj/Con_fusion-Restaurant.git"
  },
  "bugs": {
    "url": "https://github.com/mehtadhiraj/Con_fusion-Restaurant/issues"
  },
  "homepage": "https://github.com/mehtadhiraj/Con_fusion-Restaurant#readme",
  "description": ""
}
